{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [2, "tab"],
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "react/prefer-stateless-function": 0,
        "react/prop-types":0,
        "object-curly-spacing": 0,
        "linebreak-style": 0,
        "comma-dangle":0,
        "react/jsx-indent": 0,
        "no-tabs":0,
        "key-spacing": 0
    }
}